10 CLS
20 X0=100
30 Y0=100
40 R=60
50 X=R
60 Y=0
70 DIS=1-X
80 WHILE X >= Y
90 PSET (X0+X, Y0+Y), 255
100 PSET (X0-X, Y0+Y), 255
110 PSET (X0+X, Y0-Y), 255
120 PSET (X0-X, Y0-Y), 255
130 PSET (X0+Y, Y0+X), 255
140 PSET (X0-Y, Y0+X), 255
150 PSET (X0+Y, Y0-X), 255
160 PSET (X0-Y, Y0-X), 255
170 Y=Y+1
180 IF DIS <= 0 THEN DIS = DIS + Y * 2 + 1 ELSE X = X -1; DIS = DIS + (Y-X) * 2 + 1
190 WEND
