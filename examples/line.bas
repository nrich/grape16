10 X0=220; Y0=20; X1=180; Y1=110
20 STEEP=0
30 IF ABS(X0-X1) < ABS(Y0-Y1) THEN SWAP X0, Y0; SWAP X1, Y1; STEEP=1
40 IF X0>X1 THEN SWAP  X0, X1; SWAP Y0, Y1
50 DX=X1-X0
60 DY=Y1-Y0
70 DERROR2=ABS(DY) * 2
80 ERROR2=0
90 Y=Y0
100 FOR X=X0 TO X1
110 IF STEEP THEN PSET(Y,X),255 ELSE PSET(X,Y),255
120 ERROR2=ERROR2+DERROR2
130 IF ERROR2 > DX THEN ERROR2 = ERROR2-DX*2; IF Y1>Y0 THEN Y=Y+1 ELSE Y=Y-1
140 NEXT
